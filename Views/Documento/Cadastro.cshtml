@model Documento
@{
    ViewData["Title"] = "Cadastro";
}

<div class="row">
    <div class="col-md-12">
        <form method="POST" enctype="multipart/form-data" asp-action="Cadastro">
        
    <div class="form-group">
        <label asp-for="Codigo">Codigo</label>
        <input asp-for="Codigo">
    </div>

    <div class="form-group" >
        <label asp-for="Titulo">Titulo</label>
        <input asp-for="Titulo" required>
    </div>

@*<p><b>teste CheckBox</b></p>
Azul: <input type="checkbox"> <br/>*@

    <div class="form-group">
        <label>Processo</label>
        <select asp-for="Processo" required> 
            <option value="Andamento">Andamento</option>
            <option value="Concluido">Concluido</option>
            <option value="Nao Iniciado">Nao Iniciado</option>
            <option value="Outras Opcoes">Outras Opcoes </option>
        </select>
    </div>

    <div class="form-group">
        <label asp-for="Categoria">Categoria</label>
        <input asp-for="Categoria" required>
    </div>

    <div class="form-group">
        <label asp-for="file">Arquivo</label>
        <input type="file" asp-for="file" required>
    </div>

    <div class="form-group">
        <input type="submit" value="Cadastrar">
    </div>

</form>
    </div>
</div>

<p>
    @ViewBag.Mensagem
</p>